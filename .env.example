# Flutter Environment Configuration
# Copy this file to .env and set your local development values

# Test Mode Configuration (for development only)
# Set to 'true' to enable test mode with pre-filled credentials
TEST_MODE=false
ADMIN_TEST_MODE=false

# Test Credentials (only used if TEST_MODE or ADMIN_TEST_MODE is true)
# These should NEVER contain real production credentials
# Use only for local development with test accounts
TEST_USERNAME=
TEST_PASSWORD=
ADMIN_USERNAME=
ADMIN_PASSWORD=

# Note: These environment variables are only loaded in debug mode for non-web builds
# For security, never commit real credentials to version control
# For production builds, these values are ignored
